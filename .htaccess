<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Serve files from the public/storage directory (where the symbolic link points)
    RewriteRule ^storage/(.*)$ public/storage/$1 [L]  # Correct the path to storage

    # General rewrite rule to ensure requests are routed correctly to the public folder
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

<IfModule php7_module>
